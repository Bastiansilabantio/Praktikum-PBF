      "source": [
        "def f(x):\n",
        "  return 4*x\n",
        "\n",
        "def g(x):\n",
        "  return x*x\n",
        "\n",
        "def h(x):\n",
        "  return 3+x\n",
        "\n",
        "#composition\n",
        "h(g(f(4)))"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "def compose(f1, f2):\n",
        "  return lambda v: f1(f2(v))\n",
        "\n",
        "hgf = compose(h, compose(g,f))\n",
        "hgf(3)"
      ],
